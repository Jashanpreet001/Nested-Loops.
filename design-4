//

#include <stdio.h>


int main(){
    
    int i,j;
    
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=i;j++)
        { printf("%d",6-j);}
        
         printf("\n");
    }
   
    return 0;

}
