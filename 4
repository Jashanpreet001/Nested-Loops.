#include <stdio.h>


int main(){
    
    int i,j,lines;
    
    printf("enter the  no. of lines\n");
    scanf("%d",&lines);
    
    for(i=1;i<=lines;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("%c",j+64);
            
            }
        printf("\n");
    }
    
    
    
    
    return 0;
}
